<ErrorMessages moduleName="*OSBclient">
    <error num="100" severity="1">OSBclient: Request for the telephony
    integration to wait for a call failed </error>
    <error num="102" severity="1">OSBclient: Invalid extension suffix
    for a configuration parameter name </error>
    <error num="103" severity="1">OSBclient: Invalid type for a
    configuration parameter, VXIInteger required </error>
    <error num="105" severity="1">OSBclient: Unknown log implementation
    for enabling diagnostic tags </error>
    <error num="106" severity="1">OSBclient: Invalid tag ID suffix for
    a configuration parameter, integer greater then zero required </error>
    <error num="107" severity="1">OSBclient: Out of memory </error>
    <error num="108" severity="1">OSBclient: Cannot open configuration
    file </error>
    <error num="109" severity="1">OSBclient: Configuration file read
    failed </error>
    <error num="110" severity="1">OSBclient: Configuration file
    references an environment variable that is not set </error>
    <error num="111" severity="1">OSBclient: Required configuration
    parameter not set </error>
    <error num="112" severity="1">OSBclient: Invalid type for
    configuration parameter </error>

    <error num="200" severity="2">OSBclient: Function call failed </error>

    <error num="300" severity="3">OSBclient: Configuration file syntax
    error </error>
    <error num="301" severity="3">OSBclient: Unsupported configuration
    file data </error>
</ErrorMessages>
